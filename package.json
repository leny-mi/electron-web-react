{
  "name": "synthchron-electron",
  "private": true,
  "homepage": "https://github.com/synthchron/synthchron",
  "version": "1.0.0",
  "description": "An Electron application with React and TypeScript",
  "main": "./out/main/index.js",
  "author": "SEA",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "electron-start": "electron-vite preview",
    "electron-dev": "electron-vite dev",
    "electron-build": "npm run typecheck && electron-vite build",
    "electron-postinstall": "electron-builder install-app-deps",
    "electron-build:win": "npm run build && electron-builder --win --config",
    "electron-build:mac": "electron-vite build && electron-builder --mac --config",
    "electron-build:linux": "electron-vite build && electron-builder --linux --config",
    "web-dev": "vite",
    "web-build": "tsc && vite build",
    "web-preview": "vite preview"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^1.0.3",
    "@electron-toolkit/utils": "^1.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/notarize": "^1.2.3",
    "@types/node": "^18.14.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "@vitejs/plugin-react-swc": "^3.2.0",
    "electron": "21.3.3",
    "electron-builder": "^23.6.0",
    "electron-vite": "^1.0.17",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "is-electron": "^2.2.1",
    "prettier": "^2.8.2",
    "typescript": "^4.9.4",
    "vite": "^4.1.2"
  }
}
